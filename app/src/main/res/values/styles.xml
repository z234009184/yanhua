<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:statusBarColor" tools:targetApi="lollipop">@android:color/transparent</item>
        <item name="android:navigationBarColor" tools:targetApi="lollipop">@color/red</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="OnClick">
        <item name="android:clickable">true</item>
        <item name="android:onClick">onClick</item>
    </style>
    <style name="LunchTheme" parent="android:Theme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <!--将Activity的Theme设置成透明-->
        <!--<item name="android:windowIsTranslucent">true</item>-->
        <!--下面这句如果你加上觉得好看就加，不好看就不加咯-->
        <!--<item name="android:navigationBarColor">@color/transparent</item>-->
        <item name="android:statusBarColor" tools:ignore="NewApi">@android:color/transparent</item>
        <item name="android:windowBackground">@drawable/lunch_img</item>
    </style>

</resources>
